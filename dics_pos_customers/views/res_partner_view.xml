<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="dics_view_customer_form" model="ir.ui.view">
            <field name="name">DICS Customer Form</field>
            <field name="model">res.partner</field>
            <field name="inherit_id" ref="base.view_partner_form"/>
            <field name="arch" type="xml">
                <xpath expr="//notebook" position="inside">
                    <page string="Point of Sales">
                        <group>
                            <field name="is_pos_customer"/>
                        </group>
                    </page>
                </xpath>
            </field>
        </record>
    </data>
</odoo>